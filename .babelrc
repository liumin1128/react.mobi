{
  "presets": [
    [
      "env", {
        "modules": false,
        "targets": {
          "browsers": [
            "last 2 versions",
            "ie >= 7"
          ]
        }
      }
    ],
    "next/babel"
  ],
  "env": {
    "development": {
      "plugins": [
        "transform-decorators-legacy",
        "transform-runtime",
        ["lodash", { "id": "recompact" }]
      ]
    },
    "production": {
      "plugins": [
        "transform-decorators-legacy",
        "transform-remove-console",
        "transform-runtime",
        ["lodash", { "id": "recompact" }]
      ]
    }
  }
}
