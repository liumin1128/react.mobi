{
  "presets": [
    "@babel/env",
    "next/babel"
  ],
  "plugins": [
    "@babel/transform-runtime",
    "transform-decorators-legacy",
    ["lodash", { "id": "recompact" }]
  ],
  "env": {
    "production": {
      "presets": ["minify"],
      "plugins": [
        "transform-remove-console"
      ]
    }
  }
}
