{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "browsers": ["last 2 versions", "ie >= 7"]}
      }
    ],
    ["@babel/preset-stage-2",{
      "decoratorsLegacy": true
    }],
    "next/babel"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    ["lodash", { "id": "recompact" }]
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-remove-console"
      ]
    }
  }
}
